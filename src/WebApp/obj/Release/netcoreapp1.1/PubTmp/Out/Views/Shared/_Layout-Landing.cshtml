            {
                var images = (List<string>)ViewData["Images"];
                foreach (var i in images)
                {
                <meta property="og:image" content="http://www.artbyhue.com/@i" />
            }
        }
        else
        {
            <meta property="og:image" content="http://www.artbyhue.com/images/artbyhuesamples.jpg" />
        }
        <meta property="og:description"
              content="@(ViewData["PageDesc"] == null ? "Easily match beautiful art to your decor colors." : ViewData["PageDesc"])" />
