        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>Art By Hue - Fine Art Print Search For Any Decor</title>
        <meta name="description" content="@ViewData["PageDesc"]">
        <link rel="canonical" href="http://www.artbyhue.com" />

        <meta property="fb:app_id" content="227109114316823" />
        <meta property="og:type" content="@(ViewData["Type"] == null ? "website" : ViewData["Type"])" />
